<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="{{ asset('asset/node_modules/bootstrap/dist/css/bootstrap.css') }}">
    <title>Document</title>
</head>
<body>
<table class="table">
    {% for note in tab %}
    <tr>
        <td>
            {{ note }}
        </td>
    </tr>
</table>
{% endfor %}
<div class="alert alert-info">
    <p>Nombre de note : {{ tab|length }}</p>
    <p>la note minimale  : {{ min(tab)}}</p>
    <p>la note maximale  : {{ max(tab)}}</p>
</div>
</body>
</html>